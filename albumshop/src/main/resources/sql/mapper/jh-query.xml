<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="bangshop.music.model.dao.OrderStorageMapper">
    <select id="findStoreStorage" parameterType="String" resultType="bangshop.music.model.dto.AlbumStorageDTO">
        select * from album_storage where storage_no = #{number}
    </select>

    <select id="findOrder" resultType="bangshop.music.model.dto.OrderDTO">
        select * from orders
    </select>

    <select id="findStorageStock" parameterType="String" resultType="bangshop.music.model.dto.AlbumStorageDTO">
        select * from album_storage where storage_no = #{number}
    </select>
</mapper>
